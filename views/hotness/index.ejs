<div class="section intro">
	<h1 class="intro__heading"><span>The hotness</span></h1>
</div>

<div class="section hotness-list">
	<ul>
	<% items.item.forEach(function (item) { %>
		<li>
			<a data-size="1/1" href="http://www.boardgamegeek.com/boardgame/<%= item.id %>">
				<h2 data-size="1/2" data-order="2"><%= item.name.value %></h2>
				<img data-size="1/2" data-order="1" src="<%= item.thumbnail.value %>" alt="" />
			</a>
		</li>
	<% }) %>
	</ul>
</div>